{
   "action": {
      "default_icon": {
         "128": "icons/128x128.png",
         "16": "icons/16x16.png",
         "32": "icons/32x32.png",
         "48": "icons/48x48.png"
      }
   },
   "background": {
      "service_worker": "libs/background.bundle.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "static/content_scripts/main.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_idle"
   }, {
      "all_frames": true,
      "js": [ "libs/inject.bundle.js" ],
      "match_about_blank": true,
      "match_origin_as_fallback": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';script-src-elem 'self' "
   },
   "description": "Best Proxy Switcher and Manager For Chrome, Firefox, Opera, Edge, Safari",
   "host_permissions": [ "https://*/*", "http://*/*" ],
   "icons": {
      "128": "icons/128x128.png",
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "ZingProxy - Proxy Switcher and Manager",
   "optional_permissions": [ "privacy" ],
   "permissions": [ "tabs", "scripting", "sidePanel", "proxy", "webRequest", "webRequestAuthProvider", "storage", "browsingData", "declarativeNetRequest" ],
   "short_name": "ZingProxy",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0.3",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "static/*", "flags/*", "pages/*", "fonts/*", "images/*", "scripts/*", "gifs/*", "libs/*" ]
   } ]
}
